{
	"restartable": "rs",
	"ignore": [".git", "node_modules/", "dist/", "coverage/", "src/**/*.spec.ts"],
	"watch": ["src", "../server/src", "../auth/src", "../server-ai/src"],
	"exec": "yarn ts-node -r tsconfig-paths/register src/main.ts",
	"env": {
		"NODE_ENV": "development",
		"LOGGER_LEVEL": "debug"
	},
	"ext": "ts"
}
