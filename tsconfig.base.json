{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2022",
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["dom", "es2020", "esnext.array"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@metad/adapter": ["packages/adapter/src/index.ts"],
      "@metad/analytics": ["packages/analytics/src/index.ts"],
      "@metad/cloud/auth": ["libs/apps/auth/src/index.ts"],
      "@metad/cloud/indicator-market": ["libs/apps/indicator-market/src/index.ts"],
      "@metad/cloud/indicator-market/*": ["libs/apps/indicator-market/*"],
      "@metad/cloud/state": ["libs/apps/state/src/index.ts"],
      "@metad/component-store": ["libs/component-store/src/index.ts"],
      "@metad/components": ["libs/component-angular/src/index.ts"],
      "@metad/components/*": ["libs/component-angular/*"],
      "@metad/contracts": ["packages/contracts/src/index.ts"],
      "@metad/copilot": ["packages/copilot/src/index.ts"],
      "@metad/copilot-angular": ["packages/copilot-angular/src/index.ts"],
      "@metad/core": ["libs/core-angular/src/index.ts"],
      "@metad/formly": ["libs/formly/src/index.ts"],
      "@metad/formly/*": ["libs/formly/*"],
      "@metad/material-theme": ["libs/material-theme/src/index.ts"],
      "@metad/ocap-angular": ["packages/angular/src/index.ts"],
      "@metad/ocap-angular/*": ["packages/angular/*"],
      "@metad/ocap-core": ["packages/core/src/index.ts"],
      "@metad/ocap-duckdb": ["packages/duckdb/src/index.ts"],
      "@metad/ocap-echarts": ["packages/echarts/src/index.ts"],
      "@metad/ocap-sql": ["packages/sql/src/index.ts"],
      "@metad/ocap-xmla": ["packages/xmla/src/index.ts"],
      "@metad/server-auth": ["packages/auth/src/index.ts"],
      "@metad/server-common": ["packages/common/src/index.ts"],
      "@metad/server-config": ["packages/config/src/index.ts"],
      "@metad/server-core": ["packages/server/src/index.ts"],
      "@metad/store": ["packages/store/src/index.ts"],
      "@metad/story": ["libs/story-angular/src/index.ts"],
      "@metad/story/*": ["libs/story-angular/*"],
      "ngx-echarts": ["packages/ngx-echarts/src/public-api"]
    },
    "preserveConstEnums": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": false,
    "esModuleInterop": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noPropertyAccessFromIndexSignature": false
  },
  "exclude": ["node_modules", "tmp"]
}
